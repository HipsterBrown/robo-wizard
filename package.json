{
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/robo-wizard.umd.js",
  "module": "./dist/robo-wizard.es.js",
  "exports": {
    ".": {
      "import": "./dist/robo-wizard.es.js",
      "require": "./dist/robo-wizard.umd.js"
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite build --watch",
    "build": "vite build",
    "build:docs": "typedoc --out _site src/index.ts",
    "test": "vitest",
    "lint": "tsdx lint",
    "prepare": "vite build",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "robo-wizard",
  "author": "HipsterBrown",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@vitest/ui": "^0.10.0",
    "husky": "^7.0.4",
    "semantic-release": "^19.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3",
    "vite": "^2.9.6",
    "vite-plugin-dts": "^1.1.1",
    "vitest": "^0.10.0"
  },
  "dependencies": {
    "@xstate/fsm": "^1.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HipsterBrown/robo-wizard.git"
  },
  "release": {
    "branches": [
      {
        "name": "main"
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/github",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "pnpm": {
    "overrides": {
      "node-notifier@<8.0.1": ">=8.0.1",
      "marked@<4.0.10": ">=4.0.10"
    }
  }
}
