{
  "extends": "../../tsconfig.json",
  "include": [
    "./src",
    "./types"
  ],
  "exclude": [
    "node_modules",
    "test",
    "dist"
  ],
  "compilerOptions": {
    "rootDir": "src",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2019"
    ],
    "jsx": "react-jsx",
    "target": "es2020",
    "skipLibCheck": true
  }
}
